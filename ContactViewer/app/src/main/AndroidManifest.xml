<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tunnelsnakes.contactviewer" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.tunnelsnakes.contactviewer.IndividualContactActivity"
            android:label="@string/title_activity_add_edit_contact" >
        </activity>
        <activity
            android:name="com.tunnelsnakes.contactviewer.ContactsListActivity"
            android:label="@string/title_activity_contacts_list">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>
